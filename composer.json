{
    "name": "api-scout/api-scout",
    "description": "A library with a few tools, to auto document your API",
    "keywords": [
        "symfony",
        "api"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Marvin Courcier",
            "email": "marvincourcier.dev@gmail.com"
        },
        {
            "name": "Community",
            "homepage": "https://github.com/api-scout/api-scout/contributors"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/expression-language": "^6.3|^7.0",
        "symfony/http-foundation": "^6.3|^7.0",
        "symfony/http-kernel": "^6.3|^7.0",
        "symfony/mime": "^6.3|^7.0",
        "symfony/property-access": "^6.3|^7.0",
        "symfony/property-info": "^6.3|^7.0",
        "symfony/serializer": "^6.3|^7.0",
        "willdurand/negotiation": "^3.1"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.8",
        "beberlei/assert": "^3.3",
        "behat/behat": "^3.13",
        "behat/mink": "^1.10",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/orm": "^2.16",
        "friends-of-behat/mink-browserkit-driver": "^1.6",
        "friends-of-behat/mink-extension": "^2.7",
        "friends-of-behat/symfony-extension": "^2.4",
        "phpunit/phpunit": "^9.3",
        "ramsey/uuid": "^4.7",
        "symfony/asset": "^6.3|^7.0",
        "symfony/console": "^6.3|^7.0",
        "symfony/doctrine-bridge": "^6.3|^7.0",
        "symfony/dotenv": "^6.3|^7.0",
        "symfony/flex": "^2.3",
        "symfony/framework-bundle": "^6.3|^7.0",
        "symfony/http-client": "^6.3|^7.0",
        "symfony/intl": "^6.3|^7.0",
        "symfony/phpunit-bridge": "^6.3|^7.0",
        "symfony/process": "^6.3|^7.0",
        "symfony/routing": "^6.3|^7.0",
        "symfony/runtime": "^6.3|^7.0",
        "symfony/twig-bundle": "^6.3|^7.0",
        "symfony/uid": "^6.3|^7.0",
        "symfony/validator": "^6.3|^7.0",
        "symfony/yaml": "^6.3|^7.0",
        "twig/twig": "^3.6"
    },
    "suggest": {
        "ramsey/uuid": "To support Ramsey's UUID identifiers.",
        "symfony/asset": "To support Symfony assets",
        "symfony/uid": "To support Symfony UUID/ULID identifiers.",
        "symfony/twig-bundle": "To use the Swagger UI integration."
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "bamarni/composer-bin-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ApiScout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApiScout\\Tests\\": "tests/"
        }
    }
}
